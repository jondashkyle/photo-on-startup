<main class="">
  <% for (var slug in public._data) { %>
    <% var photo = public._data[slug] %>
    <div class="c6">
      <figure>
        <img data-src="<%- photo.image %>">
        <figcaption>
          <%- photo.date %><br/>
          <%- photo.time %>
        </figcaption>
      </figure>
    </div>
  <% } %>
</main>