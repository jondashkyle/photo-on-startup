<main class="p1">
  <% for (var slug in public._data) { %>
    <% var photo = public._data[slug] %>
    <div class="c6 p1">
      <figure>
        <img src="<%- photo.image %>">
        <figcaption>
          <%- photo.date %><br/>
          <%- photo.time %>
        </figcaption>
      </figure>
    </div>
  <% } %>
</main>